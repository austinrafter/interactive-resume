{
  "name": "interactive-resume",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "fix-with-prettier": "prettier --cache 'src/**/*.ts' 'src/**/*.tsx' 'src/**/*.mdx' 'backend/**/*.ts' --write",
    "fix-with-eslint": "eslint --cache --ext=js,jsx,ts,tsx src --fix",
    "autolint": "npm run fix-with-eslint && npm run fix-with-prettier",
    "prepare": "husky install",
    "test": "vitest",
    "coverage": "vitest run --coverage",

    "start-backend": "tsc --project backend && node ./backend/dist/http-server.js",
    "dev-backend": "tsc-watch --project backend --onSuccess \"node ./backend/dist/http-server.js\"",
    "lint-backend": "eslint --ext .ts --fix backend/",
    "prettier-backend": "prettier --write backend/**/*.ts"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0-rc.2",
    "@mdx-js/react": "^2.3.0",
    "@pinecone-database/pinecone": "^0.0.9",
    "classnames": "^2.3.2",
    "cohere-ai": "^5.0.2",
    "crypto-js": "^4.1.1",
    "langchain": "^0.0.75",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.4",
    "react-router-dom": "^6.11.1",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/localforage": "^0.0.34",
    "@types/lodash": "^4.14.194",
    "@types/match-sorter": "^6.0.0",
    "@types/mdx": "^2.0.4",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.0.11",
    "@types/sort-by": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "express": "^4.18.2",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "less": "^4.1.3",
    "lodash": "^4.17.21",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-mdx": "^3.5.11",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
