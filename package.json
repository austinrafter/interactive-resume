{
  "name": "interactive-resume",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "fix-with-prettier": "prettier --cache 'src/**/*.ts' 'src/**/*.tsx' --write",
    "fix-with-eslint": "eslint --cache --ext=js,jsx,ts,tsx src --fix",
    "autolint": "npm run fix-with-eslint && npm run fix-with-prettier",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.0.0-rc.2",
    "@mdx-js/react": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactjs-popup": "^2.0.5"
  },
  "devDependencies": {
    "@mdx-js/rollup": "^2.3.0",
    "@types/mdx": "^2.0.4",
    "@types/node": "^18.16.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "husky": "^8.0.0",
    "path": "^0.12.7",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-mdx": "^3.5.11"
  }
}
